<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

     <style name="applicant_text_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_gravity">left|center</item>
        <item name="android:textColor">#666666</item>
        <item name="android:textSize">15sp</item>
        <item name="android:paddingLeft">10dp</item>
        
    </style>
    
    <!-- 背景边距 -->
    <style name="background_style">
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
    </style>
    <!-- 小工具 -->
    <style name="tool_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">2</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">normal</item>
    </style>
    <!-- 工具 -->
    <style name="tool_style_list">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">2</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:textSize">14dp</item>
    </style>

    <style name="tool_style_listin">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">2</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:textSize">14dp</item>
    </style>

    <!-- 我的保险详情列 -->
    <style name="applicant_style">
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginTop">10dip</item>
       <item name="android:layout_marginRight">10dp</item>
        <item name="android:gravity">center_vertical</item>
        
    </style>
    

        
    
    <!--注册ClearEditText-->
    <style name="register_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:textColor">#666666</item>
        <item name="android:textSize">15sp</item>
        <item name="android:background">@drawable/common_textarea</item>
        <item name="android:paddingLeft">10dp</item>
        
    </style>
        <!--新版注册ClearEditText-->
    <style name="register_style_new">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1.5</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textColor">#666666</item>
        <item name="android:textSize">15sp</item>
        <item name="android:background">@drawable/common_textarea_new</item>
        <item name="android:paddingLeft">10dp</item>
        
    </style>

    <!-- 我的保险副标题栏 -->
    <style name="applicant_sectitle_style">
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:layout_marginTop">15dp</item>
    </style>

    <!-- 我的保险副标题文字 -->
    <style name="applicant_sectitletext_style">
        <item name="android:layout_marginLeft">25dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <!-- 我的保险主标题文字 -->
    <style name="applicant_firtitletext_style">
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16dp</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <!-- 我的保险主界面 -->
    <style name="applicant_mainlayout_style">
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
    </style>

    <!-- 投保人资料变更 通用 -->
    <style name="applicant_editext_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:singleLine">true</item>
    </style>

    <!-- 投保人资料变更 通用 -->
    <style name="applicant_spinner_style">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="notitle">
        <item name="android:windowNoTitle">true</item>
    </style>

    <!-- tab -->
    <style name="tab_bottom_main">
        <item name="android:textSize">@dimen/bottom_tab_font_size</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingTop">@dimen/bottom_tab_padding_up</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
        <item name="android:singleLine">true</item>
        <item name="android:drawablePadding">@dimen/bottom_tab_padding_drawable</item>
        <item name="android:layout_weight">1.0</item>
    </style>

    <!-- iphone -->
    <style name="iPone_dialogbg" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">false</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@color/alpha_00</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">false</item>
        <!-- 模糊 -->
    </style>

    <!-- 自动播放广告 -->
    <style name="dot_style">
        <item name="android:layout_width">5dip</item>
        <item name="android:layout_height">5dip</item>
        <item name="android:background">@drawable/common_textarea_new</item>
        <item name="android:layout_marginLeft">1.5dip</item>
        <item name="android:layout_marginRight">1.5dip</item>
    </style>

